<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- This is a simplified representation of the scale lines -->
    <!-- In a real app, you would create a custom view to draw all the tick marks -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#00000000" />
            <stroke android:color="#80FFFFFF" android:width="1dp" android:dashWidth="1dp" android:dashGap="3dp" />
        </shape>
    </item>
</layer-list>